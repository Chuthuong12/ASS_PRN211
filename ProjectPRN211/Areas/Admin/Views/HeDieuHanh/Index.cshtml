@using Project_Testing.Models
@model List<Hedieuhanh>

@{
    Layout = "_LayoutAdmin";
}

<h2>Quản lý hệ điều hành</h2>

<p>
    @Html.ActionLink("Thêm mới hệ điều hành", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Mã hệ điều hành
        </th>
        <th>
            Tên hệ điều hành
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @item.Mahdh
    </td>
    <td>
        @item.Tenhdh
    </td>
    <td>
        @Html.ActionLink("Sửa", "Edit", new { id = item.Mahdh }) |
        @Html.ActionLink("Chi tiết", "Details", new { id = item.Mahdh }) |
        @Html.ActionLink("Xóa ", "Delete", new { id = item.Mahdh })
    </td>
</tr>
}

</table>
